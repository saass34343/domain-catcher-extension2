// manifest.json
{
  "manifest_version": 3,
  "name": "Domain Catcher",
  "version": "1.0",
  "description": "Catch good domain names from expireddomains.net based on specific criteria",
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
  }
  "content_scripts": [
    {
      "matches": ["*://www.expireddomains.net/*"],
      "js": ["content.js"]
    }
  ],
  }
}
